{
  "name": "music-tools",
  "version": "1.0.0",
  "description": "Web Application built on AngularJS to access the MusicTools API",
  "keywords": [
    "angular",
    "web",
    "ui",
    "music"
  ],
  "authors": [
    "Axel Penin"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Elorfin/MusicTools.git"
  },
  "bugs": {
    "url": "https://github.com/Elorfin/MusicTools/issues"
  },
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "lite": "lite-server",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "build": "tsc && grunt build"
  },
  "dependencies": {
    "bootstrap": "^4.0.0-alpha.2",
    "font-awesome": "^4.6",

    "@angular/common":  "2.0.0-rc.2",
    "@angular/compiler":  "2.0.0-rc.2",
    "@angular/core":  "2.0.0-rc.2",
    "@angular/http":  "2.0.0-rc.2",
    "@angular/platform-browser":  "2.0.0-rc.2",
    "@angular/platform-browser-dynamic":  "2.0.0-rc.2",
    "@angular/router":  "2.0.0-rc.2",
    "@angular/router-deprecated":  "2.0.0-rc.2",
    "@angular/upgrade":  "2.0.0-rc.2",

    "systemjs": "0.19.27",
    "es6-shim": "^0.35.0",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "grunt": "~1.0",
    "grunt-cli": "~1.2",
    "grunt-contrib-copy": "~1.0",
    "grunt-contrib-sass": "~1.0",

    "concurrently": "^2.0.0",
    "lite-server": "^2.2.0",
    "typescript": "^1.8.10",
    "typings": "^0.8.1"
  }
}
